{
    "name": "YouTube Blacklist",
    "short_name": "youtube-blacklist--extension",
    "description": "Blacklist channels on YouTube",
    "version": "0.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "scripts/content.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "web_accessible_resources": [
        "injected/watch.js"
    ],
    "permissions": [
        "*://*.youtube.com/*"
    ]
}